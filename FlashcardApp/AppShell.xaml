    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:FlashcardApp"
    Title="FlashcardApp">

    <ShellContent
        Title="Başlangıç"
        ContentTemplate="{DataTemplate local:StartPage}"
